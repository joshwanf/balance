{
  "name": "balance-backend",
  "version": "1.0.0",
  "main": "index.js",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy-prisma": "npx prisma migrate deploy",
    "build": "tsc -p tsconfig.json",
    "start": "node build/src/app.js",
    "dev": "nodemon --watch src src/app.ts",
    "_drop-schema": "echo \"DROP SCHEMA $DATABASE_SCHEMA CASCADE;\" | psql $DATABASE_LINK",
    "drop": "dotenv -- yarn run _drop-schema",
    "clear-migrations": "rm -rf prisma/migrations",
    "migrate": "npx prisma migrate dev",
    "gen": "npx prisma generate",
    "push": "npx prisma db push",
    "seed": "npx prisma db seed",
    "reseed": "npm run drop && npm run push && npm run seed"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^6.0.1",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/csurf": "^1.11.5",
    "@types/express": "^5.0.0",
    "@types/helmet": "^0.0.48",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/morgan": "^1.9.9",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csurf": "^1.10.0",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.4",
    "express": "^4.21.1",
    "express-async-errors": "^3.1.1",
    "express-validator": "^7.2.0",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "nodemon": "^3.1.7",
    "prisma": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "uuidv7": "^1.0.2"
  },
  "devDependencies": {
    "@types/node": "^22.9.3"
  }
}
